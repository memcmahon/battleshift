<%= form_for @user, url: '/register' do |f| %>
  <%= f.label :email %>
  <%= f.email_field :email%>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :password %>
  <%= f.text_field :password %>

  <%= f.label :password_confirmation %>
  <%= f.text_field :password_confirmation %>

  <%= f.submit "Submit" %>
<% end %>
